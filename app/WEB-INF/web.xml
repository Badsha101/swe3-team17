  </servlet-mapping>


  <servlet>
    <servlet-name>Centers</servlet-name>
    <servlet-class>hbv.web.Servlets.adminLoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Centers</servlet-name>
    <url-pattern>/centers</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Centerlist</servlet-name>
    <servlet-class>hbv.web.Servlets.adminLoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Centerlist</servlet-name>
    <url-pattern>/centerlist</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Vaccines</servlet-name>
    <servlet-class>hbv.web.Servlets.adminLoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Vaccines</servlet-name>
    <url-pattern>/vaccines</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Vaccinslist</servlet-name>
    <servlet-class>hbv.web.Servlets.adminLoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Vaccinslist</servlet-name>
    <url-pattern>/vaccinslist</url-pattern>
  </servlet-mapping>

</web-app>
